{
  "name": "@titicaca/eslint-config-triple",
  "version": "0.0.1",
  "description": "Triple's ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "prelint": "eclint check * rules/* test/*",
    "lint": "eslint .",
    "pretests-only": "node ./test/requires",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "pretest": "npm run --silent lint",
    "test": "npm run --silent tests-only",
    "link:eslint": "cd node_modules/eslint && npm link --production && cd -",
    "pretravis": "npm run link:eslint && cd ../eslint-config-airbnb-base && npm link eslint && npm install && npm link && cd - && npm link --no-save eslint-config-airbnb-base",
    "travis": "npm run --silent tests-only",
    "posttravis": "npm unlink --no-save eslint-config-airbnb-base eslint >/dev/null &"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/titicacadev/eslint-config-triple.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "triple",
    "javascript",
    "typescript",
    "styleguide",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "author": "Triple Corp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/titicacadev/eslint-config-triple/issues"
  },
  "homepage": "https://github.com/titicacadev/eslint-config-triple",
  "engines": {
    "node": ">= 6"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "confusing-browser-globals": "^1.0.8",
    "eslint": "^6.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-find-rules": "^3.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@babel/runtime": "^7.5.5",
    "babel-preset-airbnb": "^4.0.1",
    "babel-tape-runner": "^3.0.0",
    "eclint": "^2.8.1",
    "in-publish": "^2.0.0",
    "react": "^16.9.0",
    "safe-publish-latest": "^1.1.3",
    "tape": "^4.11.0"
  }
}
